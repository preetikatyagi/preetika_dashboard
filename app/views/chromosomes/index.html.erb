<h1>Listing chromosomes</h1>

<table>
  <tr>
  </tr>

<% @chromosomes.each do |chromosome| %>
  <tr>
    <td><%= link_to 'Show', chromosome %></td>
    <td><%= link_to 'Edit', edit_chromosome_path(chromosome) %></td>
    <td><%= link_to 'Destroy', chromosome, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New chromosome', new_chromosome_path %>